{
  "name": "docue/procountor-php",
  "description": "Laravel package for PHP Procountor client",
  "version": "0.0.1",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Procountor\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require": {
    "php": "^7.4|^8.0",
    "psr/http-client": "^1.0",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "vlucas/phpdotenv": "^5.3",
    "guzzlehttp/guzzle": "^7.3",
    "illuminate/support": "^8.61",
    "illuminate/console": "^8.61",
    "php-extended/php-http-message-factory-psr17": "^3.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Procountor\\Laravel\\ProcountorServiceProvider"
      ],
      "aliases": {
        "Procountor": "Procountor\\Laravel\\ProcountorFacade"
      }
    }
  }
}
